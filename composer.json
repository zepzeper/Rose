{
    "name": "rose/framework",
    "description": "The Rose Framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "wouterschiedam",
            "email": "79146984+wouterschiedam@users.noreply.github.com"
        }
    ],
    "require": {
        "monolog/monolog": "^3.8",
        "symfony/http-foundation": "^7.2",
        "nesbot/carbon": "^3.8",
        "symfony/var-dumper": "^7.2",
        "filp/whoops": "^2.17",
        "vlucas/phpdotenv": "^5.6",
        "symfony/finder": "^7.2",
        "phpoption/phpoption": "^1.9",
        "phpstan/phpstan": "^2.1",
        "twig/twig": "^3.20"
    },
    "autoload": {
        "psr-4": {
            "Rose\\": "src/"
        },
        "files": [
            "src/Support/Album/helpers.php",
            "src/Roots/System/functions.php" 
        ]
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan --configuration=phpstan.src.neon.dist --memory-limit=2G"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.69",
        "phpunit/phpunit": "^11.5",
        "mockery/mockery": "^1.6"
    }
}
